{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "lib": [
            "ESNext",
            "DOM",
            "DOM.Iterable"
        ],
        "module": "ESNext",
        "skipLibCheck": true,
        "allowJs": false,
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "noUnusedLocals": false,
        "jsx": "react-jsx",
        "baseUrl": "./",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@assets/*": [
                "src/assets/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@services/*": [
                "src/services/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@Interface/*": [
                "src/Interface/*"
            ],
            "@interfaces/*": [
                "src/interfaces/*"
            ]
        }
    }
}